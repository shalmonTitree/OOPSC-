#include <bits/stdc++.h>
using namespace std;

class Vehicle {
public:
    virtual void start() = 0;  // abstraction
    virtual ~Vehicle() {}
};

class Car : public Vehicle {
public:
    void start() override {
        cout << "Car starts with key or button" << endl;
    }
};

int main() {
    Vehicle* v = new Car();  // user knows "start", not how
    v->start();
    delete v;
    return 0;
}
